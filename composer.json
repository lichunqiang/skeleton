{
  "name": "light/skeleton",
  "description": "light's project skeleton",
  "keywords": [
    "skeleton"
  ],
  "homepage": "https://github.com/light/skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "lichunqiang",
      "email": "light-li@hotmail.com"
    }
  ],
  "require": {
    "php": "~5.6|~7.0"
  },
  "autoload": {
    "psr-4": {
      "light\\skeleton\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "light\\skeleton\\": "tests"
    }
  },
  "scripts": {
    "optimizer": "composer dump-autoload -o --apcu",
    "test": [
      "php ./vendor/bin/phpunit"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0||~6.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": "1.0-dev"
  }
}
